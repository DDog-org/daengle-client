    "prettier": "^3.2.5",
    "turbo": "^2.3.0",
    "typescript": "5.5.4",
    "typescript-eslint": "^8.15.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --config ./.prettierrc --write -u"
