  },
  "devDependencies": {
    "@daengle/eslint-config": "workspace:*",
    "@daengle/typescript-config": "workspace:*",
    "@turbo/gen": "^1.12.4",
    "@types/eslint": "^8.56.5",
    "@types/node": "^20.11.24",
    "@types/react": "18.3.0",
    "@types/react-dom": "18.3.1",
    "typescript": "5.5.4"
  }
